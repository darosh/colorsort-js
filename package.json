{
  "name": "colorsort",
  "version": "0.0.0",
  "devDependencies": {
    "@types/chroma-js": "^3.1.2",
    "@types/culori": "^4.0.1",
    "@types/js-levenshtein": "^1.1.3",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "chroma-js": "^3.2.0",
    "colorgram": "^0.2.0",
    "colorjs.io": "^0.6.0",
    "culori": "^4.0.2",
    "js-levenshtein": "^1.1.6",
    "json-stringify-pretty-compact": "^4.0.0",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "prettier": "^3.3.1",
    "three": "^0.182.0",
    "typescript": "^5.2.2",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "vuetify": "^3.11.6"
  },
  "private": true,
  "scripts": {
    "build": "tsc && vite build --outDir docs",
    "dev": "vite",
    "lint": "tsc && oxlint ./src --fix && oxfmt ./src",
    "preview": "vite preview",
    "test:all": "deno run -A npm:vitest",
    "test:bench": "deno run -A npm:vitest ./tests/bench.test.js",
    "test:compute": "deno run -A npm:vitest ./tests/compute.test.js",
    "test:fast": "deno run -A npm:vitest --exclude ./tests/compute.test.js --exclude ./tests/palettes.test.js --exclude ./tests/bench.test.js"
  },
  "type": "module"
}
